  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0",
    "area17/twill": "2.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "A17\\Twill\\API\\ServiceProvider"
      ],
      "aliases": {
        "TwillAPI": "A17\\Twill\\API\\Facade"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "A17\\Twill\\API\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  }
}
